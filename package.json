{
  "name": "quickshare-list",
  "version": "0.1.0",
  "description": "QuickShare List Telegram Mini App",
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./style.css --minify",
    "dev:css": "tailwindcss -i ./src/input.css -o ./style.css --watch",
    "build:js": "rollup -c",
    "dev:js": "rollup -c -w",
    "build": "npm run build:css && npm run build:js",
    "dev": "npm run dev:css & npm run dev:js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.9.1",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3"
  }
}
